   "source": [
    "#compute integer n+nn+nnn\n",
    "n=int(input(\"Enter the integer:\"))\n",
    "n=n\n",
    "nn=n*10+n\n",
    "nnn=n*100+nn\n",
    "result=n+nn+nnn\n",
    "print(n,\"+\",nn,\"+\",nnn,\"=\",result)"
   ]
